<?xml version="1.0" encoding="utf-8"?>
<com.redbooth.WelcomePageLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/welcome_page"
    tools:background="#68B7AB">
    <TextView
        app:view_behavior=".View.ParallaxSubtitleBehaviour"
        style="@style/text_subtitle"
        android:id="@+id/title"
        android:layout_above="@+id/subtitle"
        android:text="感谢您，使用我们的App"
         />
    <TextView
        app:view_behavior=".View.ParallaxTitleBehaviour"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        style="@style/text_title"
        android:id="@+id/subtitle"
        android:text="现在请您跟着我完成设置"
         />
</com.redbooth.WelcomePageLayout>
